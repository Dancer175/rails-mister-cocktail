<div class="create-container">
  <h2>Create a new Cocktail</h2>
    <%= simple_form_for @cocktail do |f| %>
    <%= f.input :name %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit, class: "home-button" %>
  <% end %>
  <%= link_to "back to HOME", root_path, class: "home-button" %>
</div>
