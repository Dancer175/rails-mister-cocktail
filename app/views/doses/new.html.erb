<div class="create-container">
  <h2>Create a new Dose</h2>
  <div id="create-form">
    <%= simple_form_for [@cocktail, @dose] do |f| %>
    <%= f.association :ingredient %>
    <%= f.input :description %>
    <%= f.button :submit, class: "home-button" %>
  <% end %>
  </div>
    <%= link_to "back to HOME", root_path, class: "home-button" %>
</div>


